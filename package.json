{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "React server side rendering project from scratch",
  "main": "index.js",
  "scripts": {
    "start": "WEBPACK_ENV=development webpack serve --open --config webpack.client.config.js",
    "build": "WEBPACK_ENV=production webpack --config webpack.client.config.js",
    "start:server": "nodemon --watch src --exec 'WEBPACK_ENV=development webpack --config webpack.server.config.js && node build/server/index.js' ",
    "build:server": "WEBPACK_ENV=production webpack --config webpack.server.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mnindrazaka/react-ssr.git"
  },
  "keywords": [
    "react",
    "server side rendering"
  ],
  "author": "mnindrazaka@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mnindrazaka/react-ssr/issues"
  },
  "homepage": "https://github.com/mnindrazaka/react-ssr#readme",
  "dependencies": {
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "html-webpack-plugin": "^5.3.1",
    "nodemon": "^2.0.7",
    "webpack": "^5.39.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
